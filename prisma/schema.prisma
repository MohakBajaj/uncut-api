generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                String @id @default(uuid())
  username          String @unique
  user_hash         String @unique
  group_affiliation String
}

model Admins {
  id       String @id @default(uuid())
  username String @unique
}
